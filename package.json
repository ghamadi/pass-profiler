{
  "name": "pass-profiler",
  "author": "Ghaleb Hamadi",
  "repository": {
    "type": "Git",
    "url": "https://github.com/ghamadi/pass-profiler"
  },
  "version": "0.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf out && tsup src/index.ts --format cjs,esm --dts",
    "format": "prettier --write 'src/**/*.ts'",
    "lint:ts": "tsc",
    "test": "jest",
    "script": "tsup scripts/$SCRIPT.ts --out-dir scripts/out && clear && node scripts/out/$SCRIPT.cjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/node": "^20.8.3"
  }
}
