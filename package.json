{
  "name": "pass-profiler",
  "author": "Ghaleb Hamadi",
  "repository": {
    "type": "Git",
    "url": "https://github.com/ghamadi/pass-profiler"
  },
  "version": "0.0.7",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf out && tsup src/index.ts --format cjs,esm --dts",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "tsc",
    "test": "jest",
    "test:unit": "jest unit",
    "test:e2e": "jest e2e",
    "test:prod": "NODE_ENV=production jest e2e",
    "release": "pnpm run build && changeset publish",
    "script": "tsup scripts/$npm_config_name.ts --out-dir scripts/out --silent && node scripts/out/$npm_config_name.cjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "18.17.1"
  }
}
